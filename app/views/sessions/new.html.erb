<h1>Log In</h1>
<%= form_for :login do |f| %>
  <table>
    <tr>
      <td><%= f.label :email %></td>
      <td>: <%= f.text_field :email, autofocus: true %></td>
    </tr>
    <tr>
      <td><%= f.label :password %></td>
      <td>: <%= f.password_field :password %></td>
    </tr>
    <tr>
      <td><%= f.submit "Log In" %></td>
    </tr>
  </table>
<% end %>